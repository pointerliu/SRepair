    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55877dbea520_0;
    %addi 1, 0, 2;
    %assign/vec4 v0x55877dbea520_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55877dbc2de0;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x55877dbea660_0;
    %inv;
    %store/vec4 v0x55877dbea660_0, 0, 1;
    %jmp T_1;
