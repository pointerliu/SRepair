    %store/vec4 v0x55877dbea700_0, 0, 32;
    %vpi_call 2 26 "$fwrite", v0x55877dbea700_0, "time,rstn,x\012" {0 0 0};
T_3.0 ;
    %wait E_0x55877dbd8720;
    %vpi_call 2 29 "$fwrite", v0x55877dbea700_0, "%g,%d,%d\012", $time, v0x55877dbea980_0, v0x55877dbea890_0 {0 0 0};
    %jmp T_3.0;
T_3.1 ;
    %end;
    .thread T_3;
    .scope S_0x55877dbc2de0;
T_4 ;
    %pushi/vec4 0, 0, 4;
    %split/vec4 1;
    %assign/vec4 v0x55877dbea7c0_0, 0;
    %split/vec4 1;
    %assign/vec4 v0x55877dbeaa70_0, 0;
