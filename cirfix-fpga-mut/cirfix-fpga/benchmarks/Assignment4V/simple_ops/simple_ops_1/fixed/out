    %jmp T_2.0;
T_2.1 ;
    %end;
    .thread T_2;
    .scope S_0x55f203831de0;
T_3 ;
    %pushi/vec4 0, 0, 4;
    %split/vec4 1;
    %assign/vec4 v0x55f203880f70_0, 0;
    %split/vec4 1;
    %assign/vec4 v0x55f203881140_0, 0;
