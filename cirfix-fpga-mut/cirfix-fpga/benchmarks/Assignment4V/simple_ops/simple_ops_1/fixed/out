T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x55f20386f390;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55f203881030_0, 0;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x55f203880eb0_0, 0, 4;
T_3.2 ; Top of for-loop 
